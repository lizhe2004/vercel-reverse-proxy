{
  "rewrites": [
    { "source": "/:path*", "destination": "https://api.openai.com/:path*" },
    
       { "source": "imgs?url=:path*", "destination": ":path*" ,
         "setHeaders": (res, path, proxyRes) => {
        res.setHeader('Referer', 'https://www.iqiyi.com/');
      },
        "headers": [
          {
            "key": 'Referer',
            "value": 'https://www.iqiyi.com/',
          },
        ]
    }

  ]
}
