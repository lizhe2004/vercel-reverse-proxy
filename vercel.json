{
  "rewrites": [
     { "source": "/imgs/:path*",  
       "has": [
        {
          "type": "query",
          "key": "url",
          "value":"(?<img>.*)"
        }
      ],
      "destination": "http://imgs.huahuaxia.net/:img.jpg"},
 { "source": "/:path*", "destination": "https://api.openai.com/:path*" }
  
  ]
}
