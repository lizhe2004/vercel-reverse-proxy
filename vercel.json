{
  "rewrites": [
    { "source": "/:path*", "destination": "https://api.openai.com/:path*" },
    { "source": "/imgs", 
     
           "has": [
        {
          "type": "query",
          "key": "url",
          "value":":(?<path>.*)"
        }
      ],
     
     "destination": ":path*"}

  ]
}
