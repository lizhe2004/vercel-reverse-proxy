{
  "rewrites": [
     { "source": "/imgs*",  
       "has": [
        {
          "type": "query",
          "key": "url",
          "value":"(?<path>.*)"
        }
      ],
      "destination": "http://imgs.huahuaxia.net/path*"},
 { "source": "/:path*", "destination": "https://api.openai.com/:path*" }
  
  ]
}
